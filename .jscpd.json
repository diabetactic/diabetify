{
  "threshold": 10,
  "reporters": ["console"],
  "ignore": [
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/test-*.ts",
    "**/__mocks__/**",
    "**/mocks/**",
    "**/dist/**",
    "**/coverage/**",
    "**/www/**",
    "**/android/app/build/**",
    "**/playwright/**",
    "**/maestro/**",
    "**/scripts/**",
    "**/*.config.ts",
    "**/*.config.js"
  ],
  "format": ["typescript", "javascript"],
  "minLines": 10,
  "minTokens": 100,
  "output": "./reports/jscpd"
}
