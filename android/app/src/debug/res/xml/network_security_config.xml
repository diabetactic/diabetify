<?xml version="1.0" encoding="utf-8"?>
<!--
  Network Security Configuration for DEBUG builds only
  ALLOWS ALL CLEARTEXT HTTP FOR DEVELOPMENT
-->
<network-security-config>
  <base-config cleartextTrafficPermitted="true">
    <trust-anchors>
      <certificates src="system" />
    </trust-anchors>
  </base-config>
</network-security-config>
