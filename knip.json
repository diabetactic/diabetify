{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.spec.ts",
    "**/test-*.ts",
    "**/__mocks__/**",
    "**/mocks/**",
    "**/tests/helpers/**",
    "**/tests/integration/**",
    "**/tests/integration-msw/**",
    "**/test-setup/**",
    "**/constants/**",
    "**/contracts/**",
    "**/config/mock-adapter-config.ts",
    "**/utils/type-guards.ts",
    "**/models/http-error.model.ts",
    "**/models/tidepool-auth.model.ts",
    "**/services/tidepool-auth.service.ts"
  ],
  "ignoreDependencies": [
    "@angular/language-service",
    "@analogjs/vite-plugin-angular",
    "@midscene/web"
  ],
  "ignoreBinaries": ["only-allow", "adb", "studio", "emulator", "test:visual:update"],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "unlisted": "off",
    "enumMembers": "off"
  }
}
