{
  "name": "diabetactic",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "clean": "rm -rf node_modules package-lock.json www && npm install",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:coverage": "ng test --watch=false --code-coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:maestro": "maestro test maestro/tests/",
    "test:maestro:single": "maestro test",
    "test:maestro:device": "maestro test maestro/tests/ --device",
    "test:backend-integration": "ng test --include='**/backend/*.spec.ts'",
    "test:backend-integration:headless": "ng test --include='**/backend/*.spec.ts' --watch=false --browsers=ChromeHeadless",
    "test:health": "ng test --include='**/health-check.spec.ts' --watch=false --browsers=ChromeHeadless",
    "test:health:watch": "ng test --include='**/health-check.spec.ts'",
    "test:health:script": "./scripts/run-health-checks.sh",
    "backend:start": "cd extServices/container-managing && make build",
    "backend:stop": "cd extServices/container-managing && make down",
    "backend:logs": "cd extServices/container-managing && make logs",
    "backend:test": "cd extServices/container-managing && make test",
    "backend:health": "curl -f http://localhost:8004/health && curl -f http://localhost:8002/health && curl -f http://localhost:8003/health && curl -f http://localhost:8005/health",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:css:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:all": "npm run lint && npm run lint:css",
    "lint:all:fix": "npm run lint:fix && npm run lint:css:fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
    "cap:sync": "npx cap sync",
    "cap:android": "npx cap open android",
    "cap:run:android": "npx cap run android",
    "android:env": "npx cap sync android",
    "i18n:missing": "node scripts/check-i18n-missing.js",
    "test:maestro:flow": "maestro test",
    "test:maestro:studio": "maestro studio",
    "test:maestro:continuous": "maestro test maestro/tests/ --continuous",
    "prepare": "husky",
    "postinstall": "npx cap sync"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.3.7",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@capacitor/android": "^6.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.5",
    "@capacitor/core": "^6.1.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.1",
    "@capacitor/network": "^6.0.3",
    "@capacitor/preferences": "^6.0.3",
    "@capacitor/splash-screen": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@ionic/angular": "^8.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "dexie": "^4.2.1",
    "lucide-angular": "^0.553.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/language-service": "^20.0.0",
    "@capacitor/cli": "^6.1.0",
    "@faker-js/faker": "^10.1.0",
    "@ionic/angular-toolkit": "^11.0.1",
    "@playwright/test": "^1.48.0",
    "@sparticuz/chromium-min": "^141.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.22",
    "claude-flow": "^2.7.26",
    "daisyui": "^5.4.7",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "puppeteer": "^24.29.1",
    "puppeteer-core": "^24.26.1",
    "stylelint": "^16.12.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.0"
  },
  "overrides": {
    "karma": {
      "rimraf": "3.0.2"
    }
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
