{
  "name": "diabetactic",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "start": "node scripts/start-with-env.mjs",
    "build": "ng build",
    "build:dev": "ng build",
    "build:prod": "ng build --configuration=production --aot --optimization",
    "build:mock": "ng build --configuration=mock",
    "build:heroku": "ng build --configuration=heroku",
    "build:mobile": "npm run build:prod && npx cap sync",
    "build:analyze": "ng build --stats-json && echo 'Run: npx webpack-bundle-analyzer dist/stats.json'",
    "clean": "rm -rf node_modules package-lock.json www .angular && npm install",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCI",
    "test:unit": "ng test --watch=false --browsers=ChromeHeadlessCI",
    "test:watch": "ng test",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadlessCI --code-coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:mobile": "maestro test maestro/tests/",
    "test:mobile:smoke": "maestro test maestro/tests/smoke-test.yaml",
    "test:mobile:tag": "maestro test --tag",
    "test:all": "npm run quality && npm run test:unit && npm run test:e2e",
    "test:quick": "ng test --include='**/core/**/*.spec.ts' --watch=false --browsers=ChromeHeadlessCI",
    "quality": "npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
    "mobile:sync": "npx cap sync",
    "mobile:android": "npx cap open android",
    "mobile:run": "npx cap run android",
    "mobile:build": "cd android && ./gradlew assembleDebug",
    "mobile:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "backend:start": "cd extServicesCompose/extServices/container-managing && make build",
    "backend:stop": "cd extServicesCompose/extServices/container-managing && make down",
    "backend:logs": "cd extServicesCompose/extServices/container-managing && make logs",
    "backend:health": "./scripts/backend-health.sh",
    "i18n:check": "node scripts/check-i18n-missing.js",
    "maestro:studio": "maestro studio",
    "maestro:continuous": "maestro test maestro/tests/ --continuous",
    "prepare": "husky",
    "postinstall": "[ -d www ] && npx cap sync || echo 'Skipping cap sync (no www directory yet)'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.3.7",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@aparajita/capacitor-secure-storage": "^6.0.1",
    "@capacitor/android": "^6.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.5",
    "@capacitor/core": "^6.1.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.1",
    "@capacitor/network": "^6.0.3",
    "@capacitor/preferences": "^6.0.3",
    "@capacitor/splash-screen": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@ionic/angular": "^8.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "dexie": "^4.2.1",
    "lru-cache": "^11.2.2",
    "lucide-angular": "^0.553.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/language-service": "^20.0.0",
    "@capacitor/cli": "^6.1.0",
    "@faker-js/faker": "^10.1.0",
    "@ionic/angular-toolkit": "^11.0.1",
    "@playwright/test": "^1.48.0",
    "@sparticuz/chromium-min": "^141.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.4.7",
    "eslint": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.0.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stylelint": "^16.12.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.0"
  },
  "overrides": {
    "karma": {
      "rimraf": "3.0.2"
    }
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
